{
    "manifest_version": 2,
    "name": "ND-remove-unvisible",
    "description": "Netduettoの非公開ルームをルームから除去します。 Netduetto extension for remove unvisible rooms from room list.",
    "version": "2.0.0",
    "icons": {
      "16": "icon_16.png"
    },
    "background": {
      "scripts": [
        "js/jquery.js"
      ],
      "css": ["css/popup.css"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
      },
      "default_title": "ND-remove-unvisible",
      "default_popup": "html/popup.html"
    },
    "content_scripts": [
      {
        "matches": [
          "https://www.netduetto.net/room/"
        ],
        "run_at": "document_idle",
        "css": ["css/popup.css"],
        "js": [
          "js/jquery.js",
          "js/popup.js"
        ]
      }
    ],
    "permissions": [
      "activeTab",
      "storage"
    ]
  }

